{
    "name": "dagasmart/task-schedule",
    "alias": "任务调度",
    "description": "更灵活地管理应用中的任务调度",
    "type": "library",
    "keywords": ["bizadmin", "extension","laravel","schedule"],
    "homepage": "https://github.com/dagasmart/task-schedule",
    "license": "MIT",
    "authors": [
        {
            "name": "dagasmart",
            "email": "dagasmart@qq.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "dagasmart/bizadmin": "*"
    },
    "autoload": {
        "psr-4": {
            "Dagasmart\\TaskSchedule\\": "src/"
        }
    },
    "extra": {
        "bizadmin": "Dagasmart\\TaskSchedule\\TaskScheduleServiceProvider",
        "laravel": {
            "providers": [
                "Dagasmart\\TaskSchedule\\ScheduleServiceProvider",
                "Dagasmart\\TaskSchedule\\TaskScheduleServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.5",
        "phpunit/phpunit": "^9.5"
    }
}
